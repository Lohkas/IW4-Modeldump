global proc WraithMesh_62490c3d_00_BindFunc()
{
   select -r WraithMesh_62490c3d_00;
   select -add tag_weapon;
   newSkinCluster "-toSelectedBones -mi 15 -omi true -dr 5.0 -rui false";   string $clu = findRelatedSkinCluster("WraithMesh_62490c3d_00");

   skinPercent -tv tag_weapon 1.0 $clu WraithMesh_62490c3d_00.vtx[0:1093];
}

